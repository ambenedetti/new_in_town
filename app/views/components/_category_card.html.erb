<% categories.each do |category| %>
  <%= link_to '#', data: { category: category.name, search: "true" }, class: "card-home-section" do %>
    <div class="category-card m-2">
      <div>
        <%= image_tag category.image %><br>
        <%= category.name %>
      </div>
    </div>
  <% end %>
<% end %>



